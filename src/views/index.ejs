<div id="home-page">
  <header>
    <nav><%- include("./partials/navigation");%></nav>
    <div id="hero">
      <h1>Meta Data</h1>
      <p class="sub-hero-text">
        A collection of open source and closed source images, code snippets,
        repositories, templates and more for designers, web and hybrid
        application developers!
      </p>
      <div id="call-to-action">
        <a href="/explore">Explore</a>
        <a href="/login">Add Yours</a>
      </div>
    </div>
  </header>

  <main>
    <%- include("./partials/search-form");%>
    <ul>
      <% for(const category of categories){ %>
      <li>
        <a href="<%='/r/'+category.route  %>"> <%=category.anchor %> </a>
      </li>
      <% } %>
    </ul>
  </main>
  <footer><%- include("./partials/footer"); %></footer>
</div>
