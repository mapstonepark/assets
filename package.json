{
  "version": "1.0.0",
  "description": "A collection of web games ",
  "scripts": {
    "build-sass": "sass src/public/stylesheets/index.scss dist/public/stylesheets/index.css",
    "build-ts": "tsc",
    "build": "npm run build-sass && npm run build-ts && npm run copy-static-assets",
    "copy-static-assets": "npx ts-node copy-static-assets.ts",
    "serve": "node dist/index.js",
    "start": "npm run serve",
    "watch-node": "nodemon src/index.ts",
    "watch-sass": "sass --watch src/public/stylesheets/index.scss dist/public/stylesheets/index.css",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,Typescript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "make": "bash install.sh"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.7",
    "express": "^4.18.0",
    "express-ejs-layouts": "^2.5.1",
    "jsonwebtoken": "^8.5.1",
    "mysql2": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sequelize-bcrypt": "^1.1.0",
    "sequelize-joi": "^2.0.1",
    "sqlite3": "^5.0.6",
    "typeorm": "^0.3.6"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [],
  "license": "MIT",
  "bugs": {
    "url": ""
  },
  "homepage": "",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/express-ejs-layouts": "^2.5.0",
    "@types/node": "^17.0.27",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
